!!! 5
html("xmlns\u003Afb"="http://www.facebook.com/2008/fbml", xmlns="http://www.w3.org/1999/xhtml")
	head
		!=partial('layouts/_head')
	body.hide_userlist(id=w.page_name, class=w.authenticated ? '' : 'anonymous')
		#fb-root
		!=partial('layouts/_header')
		.main
			!{body}
		!=partial('layouts/_footer')
		.clear
		- if(w.jquery)
			- if(w.config.flags.localjQuery)
				script(src="/s/external/jquery-1.5.2.min.js", type="text/javascript")
			- else
				script(src="https://ajax.googleapis.com/ajax/libs/jquery/1.5.2/jquery.min.js", type="text/javascript")
			script(src="/s/external/jquery-ui-1.8.12.custom.min.js", type="text/javascript")
		- if(w.page_js)
			- if(w.page_js == 'channel')
				script(src="/socket.io/socket.io.js")
			!{assetHelpers.assetGroupTag(w.page_js, w.user && w.user.doc)}
		- if(w.channel && !w.config.flags.noFacebook)
			script(type='text/javascript', src='//connect.facebook.net/en_US/all.js#appId=181725458505189&xfbml=1',async='true')
		!=partial('shared/_ga')
